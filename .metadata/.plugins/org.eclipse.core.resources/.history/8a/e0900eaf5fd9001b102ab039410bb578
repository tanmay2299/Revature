package operators_in_java;

public class UnaryOperators 
{

	public static void main(String[] args) 
	{
		int a = 11;
      
        System.out.println(a++);  
        
        System.out.println(++a);  
        
        System.out.println(a--);  
        
        System.out.println(--a);  
        
	}

}
