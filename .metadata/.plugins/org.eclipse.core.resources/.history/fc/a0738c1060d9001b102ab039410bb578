package operators_in_java;

public class UnaryOperators 
{

	public static void main(String[] args) 
	{
		int i = 10;
      
		i++;  
		System.out.println(i);  
		
		++i;                   
		System.out.println(i);  

		i--;
		System.out.println(i);
		System.out.println(++i);  
	 
		System.out.println(i++);  

		System.out.println(i);  
        
	}

}
