package operators_in_java;

public class BitwiseOperators 
{

	public static void main(String[] args) 
	{
		  int a = 58; 
          int b=13; 
          
          System.out.println(a&b);  
          
          System.out.println(a|b);  
          
          System.out.println(a^b);  
          
          System.out.println(~a);  
	
	}

}
